{
  "name": "Open bulk URLs",
  "manifest_version": 2,
  "version": "1.0.0",
  "permissions": ["tabs", "cookies", "http://*/*", "https://*/*"],
  "icons": {
	"16": "icon16.png",
	"32": "icon32.png",
	"64": "icon64.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_title": "Open bulk URLs",
    "default_icon": "icon16.png",
	"default_popup": "index.html"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}